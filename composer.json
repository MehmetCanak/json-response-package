  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/MehmetCanak/json-response-package",
  "keywords" : ["json", "api", "standard", "jsonapi"],
  "require": {
    "php": ">=5.3.0",
